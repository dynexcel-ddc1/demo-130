<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="group_project_scrum_master" model="res.groups">
        <field name="name">Scrum Master</field>
        <field name="category_id" ref="base.module_category_operations_project"/>
        <field name="implied_ids" eval="[(4, ref('project.group_project_manager'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>
    
    <record id="group_project_scrum_team" model="res.groups">
        <field name="name">Scrum Team</field>
        <field name="category_id" ref="base.module_category_operations_project"/>
    </record>
</odoo>